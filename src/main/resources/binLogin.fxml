<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.TextField?>
<AnchorPane fx:id="authPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.GUI.SecondaryController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="44.0" prefWidth="458.0" text="Let's start! Insert your binance key and secret key please">
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets left="5.0" />
            </padding></Label>
        <TextField fx:id="binKeyId" layoutX="15.0" layoutY="65.0" promptText="binance key" />
        <Button layoutX="14.0" layoutY="160.0" mnemonicParsing="false" onAction="#login" text="Add new user" />
        <TextField fx:id="secretBinKeyId" layoutX="15.0" layoutY="108.0" promptText="secret key" />
    </children>
</AnchorPane>
